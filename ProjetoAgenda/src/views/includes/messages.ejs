<% if (erros.length > 0){ %>
<div class="col my-3">
    <div class="alert alert-danger">
        <% erros.forEach(erro =>{ %>
            <%= erro %><br>
       <% }) %>
    </div>
</div>

<% } %>

<% if (sucess.length > 0){ %>
<div class="col my-3">
    <div class="alert alert-success">
        <% sucess.forEach(sucess =>{ %>
            <%= sucess %><br>
       <% }) %>
    </div>
</div>

<%}%>